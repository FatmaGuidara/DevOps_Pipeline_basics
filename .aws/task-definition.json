{
    "family": "fatma-task-def",
    "containerDefinitions": [
        {
            "name": "new-container",
            "image": "fatmaguidara/my-express",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
}